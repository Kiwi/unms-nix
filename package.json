{
  "name": "unms-server",
  "version": "0.13.3",
  "description": "UNMS (Ubiquiti network management system) server in Node.js",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "npm run backup:apply && npm run migrate && node --max_old_space_size=2048 index.js",
    "backup:apply": "node ./cli/apply-backup",
    "backup:redis": "redis-dump",
    "migrate": "node ./cli/migrate.js",
    "migrate:create": "node ./cli/migrate.js create",
    "migrate:down": "node ./cli/migrate.js down",
    "lint": "eslint ./",
    "lint:fix": "npm run lint -- --fix",
    "test:dev": "jest --watch --testRegex=\"(/__tests__/.*|(\\.|/)(test|spec|perf))\\.jsx?$\"",
    "test:coverage": "jest --coverage",
    "test:ci": "JEST_JUNIT_OUTPUT=\"${REPORT_DEST:-.}/junitresults.xml\" jest --ci --testResultsProcessor=\"jest-junit\"",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "setupTestFrameworkScriptFile": "./spec/init.js"
  },
